/* chosen */
&chosen {

	/* fpga may has no lk, we need to specify display info here */
	/* fb buffer is reserved using this dts by memory node above */
	atag,videolfb-fb_base_h = <0x0>;
	atag,videolfb-fb_base_l = <0x5e605000>;
	atag,videolfb-islcmfound = <1>;
	/* because no lk, so lcm is not inited */
	atag,videolfb-islcm_inited = <0>;
	atag,videolfb-fps= <6000>;
	atag,videolfb-vramSize= <0x017bb000>;
	atag,videolfb-lcmname= "nt35695B_fhd_dsi_cmd_auo_rt5081_drv";
	atag,videolfb-project-name= "PD1901";
	/* vivo panel materiel info start */
	vivo,panel_info {
		/*
		 * No add new id until you have import driver for new panel ok!!!!!!
		 * vivo,panel-materiel-id-list = "materiel_id1", "lcm1_sf_id1 lcm1_sf_id2 lcm1_sf_id3...",
		 *				 "materiel_id2", "lcm2_sf_id1 lcm2_sf_id2 lcm2_sf_id3...",
		 *				 ...
		 *				 "materiel_idX", "lcmX_sf_id1 lcmX_sf_id2 lcmX_sf_id3...";
		 */
		vivo,panel-materiel-id-list = "1211623","0x60", /*TM+FT8006P*/
								"1211623","0x7E", 		/*TM+FT8006P*/		
								"1211624","0x61",		/*BOE+NT36525*/
								"1211626","0x63",		/*AUO panel+FT8006P */
								"1211627","0x64",		/*qunchuang+NT36525	*/
								"1211636","0x6d",		/*BOE+NT36525B */
								"1211643","0x75",		/*qunchuang+ILI9881H*/
								"1211656","0x60",		/*TM+FT8006P */
								"1211656","0x7E",		/*TM+FT8006P */
								"1211693","0x78";		/*lianchuang+FT8006P	*/
	};
	/* vivo panel materiel info end */
	};

