&odm {
	vivo-soc-codec@0 {
			compatible = "vivo,vivo-soc-codec";
			status = "okay";
	};

	vivo-audio-ktv@0 {
		compatible = "vivo,vivo-audio-ktv";
		status = "okay";
	};
};
&i2c0{

		aw87xxx_i2c@58 {
			compatible = "awinic,aw87xxx_pa";
			reg = <0x58>;
			reset-gpio = <&pio 154 0>;
			status = "okay";
		};
};
